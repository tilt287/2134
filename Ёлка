#include <iostream>
using namespace std;

int main() {
    setlocale(0, "");
    cout << "      *\033[33m       *           \033[m  *" << endl;
    cout << " \033[32m            ***                          " << endl;
    cout << "\033[m * \033[31m         *****         \033[m *           " << endl;
    cout << "    *  \033[32m    *******                    \033[m  * " << endl;
    cout << "*   \033[94m      *********    \033[m*                       " << endl;
    cout << "\033[32m         ***********           \033[m*        " << endl;
    cout << "      \033[*/m        |             " << endl;
    cout << "*********************************" << endl;

    return 0;
}
